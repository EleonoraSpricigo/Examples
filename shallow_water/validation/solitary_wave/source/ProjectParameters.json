        "start_time"                 : 0.0,
        "end_time"                   : 60.0,
        "echo_level"                 : 0
    },
    "modelers"                   : [{
        "kratos_module"              : "KratosMultiphysics",
        "modeler_name"               : "import_mdpa_modeler",
        "Parameters"                 : {
            "model_part_name"            : "main_model_part",
            "input_filename"             : "solitary_wave"
        }
    }],
    "solver_settings"  : {
        "solver_type"                : "boussinesq_solver",
