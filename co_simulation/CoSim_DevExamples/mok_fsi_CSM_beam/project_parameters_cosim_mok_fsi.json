{
    "problem_data" :
    {
    	"problem_name"    : "Mok_CSM",
        "parallel_type"   : "OpenMP",
        "start_time"   : 0.0,
        "end_time"     : 15.0,
        "print_colors" : true,
        "echo"		: 0
    },
    "solver_settings" :
    {
        "type" : "coupled_solvers.gauss_seidel_strong",
        "echo_level" : 0,
        "num_coupling_iterations" : 10,
        "predictors" : [
            {
                "type"                 : "linear_derivative_based",
                "solver"               : "structure",
                "data_name"            : "top_disp",
                "derivative_data_name" : "top_vel"
            }
        ],
        "convergence_accelerators" : [
            {
                "type"      : "mvqn",
                "solver"    : "fluid",
                "data_name" : "top_disp"
            }
        ],
        "convergence_criteria" : [
            {
                "type"          : "relative_norm_previous_residual",
                "solver"        : "fluid",
                "data_name"     : "top_disp",
                "abs_tolerance" : 1e-5,
                "rel_tolerance" : 1e-5
            }
        ],
        "data_transfer_operators" : {
            "mapper" : {
                "type" : "kratos_mapping",
                "mapper_settings" : {
                    "mapper_type" : "nearest_neighbor"
                }
            }
        },
        "coupling_sequence":
        [
            {
                "name": "fluid",
                "input_data_list"  : [],
                "output_data_list" : []
            },
            {
                "name": "structure",
                "input_data_list": [
                    {
                        "data"             : "front_force",
                        "from_solver"      : "fluid",
                        "from_solver_data" : "front_force",
                        "data_transfer_operator" : "mapper",
                        "data_transfer_operator_options" : ["swap_sign"]
                    },
                    {
                        "data"             : "top_force",
                        "from_solver"      : "fluid",
                        "from_solver_data" : "top_force",
                        "data_transfer_operator" : "mapper",
                        "data_transfer_operator_options" : ["swap_sign"]
                    },
                    {
                        "data"             : "back_force",
                        "from_solver"      : "fluid",
                        "from_solver_data" : "back_force",
                        "data_transfer_operator" : "mapper",
                        "data_transfer_operator_options" : ["swap_sign"]
                    }
                ],
                "output_data_list": [
                    {
                        "data"           : "front_disp",
                        "to_solver"      : "fluid",
                        "to_solver_data" : "front_disp",
                        "data_transfer_operator" : "mapper"
                    },
                    {
                        "data"           : "top_disp",
                        "to_solver"      : "fluid",
                        "to_solver_data" : "top_disp",
                        "data_transfer_operator" : "mapper"
                    },
                    {
                        "data"           : "back_disp",
                        "to_solver"      : "fluid",
                        "to_solver_data" : "back_disp",
                        "data_transfer_operator" : "mapper"
                    }
                ]
            }
        ],
        "solvers" :
        {
            "fluid":
            {
                "type" : "solver_wrappers.kratos.fluid_dynamics_wrapper",
                "solver_wrapper_settings" : {
                    "input_file"  : "ProjectParametersCFD"
                },
                "data" : {
                    "front_disp" : {
                        "model_part_name" : "FluidModelPart.NoSlip2D_Beam_front",
                        "dimension"	 : 2,
                        "variable_name" : "MESH_DISPLACEMENT"
                    },
                    "top_disp" : {
                        "model_part_name" : "FluidModelPart.NoSlip2D_Beam_Top",
                        "dimension"	 : 2,
                        "variable_name" : "MESH_DISPLACEMENT"
                    },
                    "back_disp" : {
                        "model_part_name" : "FluidModelPart.NoSlip2D_Beam_back",
                        "dimension"	 : 2,
                        "variable_name" : "MESH_DISPLACEMENT"
                    },
                    "front_force" : {
                        "model_part_name" : "FluidModelPart.NoSlip2D_Beam_front",
                        "dimension"	 : 2,
                        "variable_name" : "REACTION"
                    },
                    "top_force" : {
                        "model_part_name" : "FluidModelPart.NoSlip2D_Beam_Top",
                        "dimension"	 : 2,
                        "variable_name" : "REACTION"
                    },
                    "back_force" : {
                        "model_part_name" : "FluidModelPart.NoSlip2D_Beam_back",
                        "dimension"	 : 2,
                        "variable_name" : "REACTION"
                    }
                }
            },
            "structure" :
            {
                "type" : "solver_wrappers.kratos.structural_mechanics_wrapper",
                "solver_wrapper_settings" : {
                    "input_file"  : "ProjectParametersCSM"
                },
                "data" : {
                    "front_disp" : {
                        "model_part_name" : "Structure.GENERIC_Beam",
                        "dimension"	 : 3,
                        "variable_name" : "DISPLACEMENT"
                    },
                    "top_disp" : {
                        "model_part_name" : "Structure.GENERIC_Beam",
                        "dimension"	 : 3,
                        "variable_name" : "DISPLACEMENT"
                    },
                    "back_disp" : {
                        "model_part_name" : "Structure.GENERIC_Beam",
                        "dimension"	 : 3,
                        "variable_name" : "DISPLACEMENT"
                    },
                    "front_force" : {
                        "model_part_name" : "Structure.GENERIC_Beam",
                        "dimension"	 : 3,
                        "variable_name" : "POINT_LOAD"
                    },
                    "top_force" : {
                        "model_part_name" : "Structure.GENERIC_Beam",
                        "dimension"	 : 3,
                        "variable_name" : "POINT_LOAD"
                    },
                    "back_force" : {
                        "model_part_name" : "Structure.GENERIC_Beam",
                        "dimension"	 : 3,
                        "variable_name" : "POINT_LOAD"
                    },
                    "front_vel" : {
                        "model_part_name" : "Structure.GENERIC_Beam",
                        "dimension"	 : 3,
                        "variable_name" : "VELOCITY"
                    },
                    "top_vel" : {
                        "model_part_name" : "Structure.GENERIC_Beam",
                        "dimension"	 : 3,
                        "variable_name" : "VELOCITY"
                    },
                    "back_vel" : {
                        "model_part_name" : "Structure.GENERIC_Beam",
                        "dimension"	 : 3,
                        "variable_name" : "VELOCITY"
                    }
                }
            }
        }
    }
}
