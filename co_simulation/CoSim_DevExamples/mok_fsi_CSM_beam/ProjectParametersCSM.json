{
    "problem_data"             : {
        "problem_name"    : "Mok_CSM",
        "parallel_type"   : "OpenMP",
        "start_time"      : 0.0,
        "end_time"        : 15,
        "echo_level"      : 0
    },
    "solver_settings"          : {
        "solver_type"                        : "Dynamic",
        "model_part_name" 					 : "Structure",
        "domain_size"    		  			 : 3,
        "echo_level"                         : 0,
        "analysis_type"                      : "linear",
        "time_integration_method"            : "implicit",
        "time_stepping" : {
            "time_step"       : 0.1
        },
        "scheme_type"                        : "newmark",
        "model_import_settings"              : {
            "input_type"     : "mdpa",
            "input_filename" : "Mok_CSM"
        },
		"line_search"                        : false,
        "compute_reactions"					 : false,
        "convergence_criterion"              : "residual_criterion",
        "displacement_relative_tolerance"    : 1e-7,
        "displacement_absolute_tolerance"    : 1e-7,
        "residual_relative_tolerance"        : 1e-8,
        "residual_absolute_tolerance"        : 1e-8,
        "max_iteration"                      : 500,
        "problem_domain_sub_model_part_list" : ["Parts_Beam"],
        "processes_sub_model_part_list"      : ["DISPLACEMENT_Boundary","ROTATION_Boundary","GENERIC_Beam"],
        "rotation_dofs"                      : true
    },
    "processes":{
    "constraints_process_list" : [{
        "python_module" : "assign_vector_variable_process",
        "kratos_module" : "KratosMultiphysics",
        "help"          : "This process fixes the selected components of a given vector variable",
        "process_name"  : "AssignVectorVariableProcess",
        "Parameters"    : {
            "mesh_id"         : 0,
            "model_part_name" : "Structure.DISPLACEMENT_Boundary",
            "variable_name"   : "DISPLACEMENT",
            "constrained"      : [true, true, true],
            "value"           : [0.0,0.0,0.0]
        }

        },
        {
        "python_module" : "assign_vector_variable_process",
        "kratos_module" : "KratosMultiphysics",
        "help"          : "This process fixes the selected components of a given vector variable",
        "process_name"  : "AssignVectorVariableProcess",
        "Parameters"    : {
            "mesh_id"         : 0,
            "model_part_name" : "Structure.GENERIC_Beam",
            "variable_name"   : "DISPLACEMENT",
            "constrained"      : [false, false, false],
            "value"           : [0.0,0.0,0.0]
        }

        },
        {
        "python_module" : "assign_vector_variable_process",
        "kratos_module" : "KratosMultiphysics",
        "help"          : "This process fixes the selected components of a given vector variable",
        "process_name"  : "AssignVectorVariableProcess",
        "Parameters"    : {
            "mesh_id"         : 0,
            "model_part_name" : "Structure.ROTATION_Boundary",
            "variable_name"   : "ROTATION",
            "constrained"      : [true, true, true],
            "value"           : [0.0,0.0,0.0]
        }

    }],
    "loads_process_list"       : [{
        "python_module"   : "process_factory",
        "kratos_module" : "KratosMultiphysics",
        "check"                 : "DirectorVectorNonZero direction",
        "help"                  : "This process ",
        "process_name"          : "ApplyConstantVectorValueProcess",
        "Parameters"            : {
            "mesh_id"         : 0,
            "model_part_name" : "Structure.GENERIC_Beam",
            "variable_name"   : "VOLUME_ACCELERATION",
            "modulus"          : 0.0,
            "direction"       : [0.0,-1.0,0.0]
        }
    },
        {
        "python_module"   : "process_factory",
        "kratos_module" : "KratosMultiphysics",
        "check"                 : "DirectorVectorNonZero direction",
        "help"                  : "This process ",
        "process_name"          : "ApplyConstantVectorValueProcess",
        "Parameters"            : {
            "mesh_id"         : 0,
            "model_part_name" : "Structure.GENERIC_Beam",
            "variable_name"   : "POINT_LOAD",
            "modulus"          : 0.00,
            "direction"       : [1.0,1.0,1.0]
        }
    },{
        "python_module"  : "point_output_process",
        "kratos_module"  : "KratosMultiphysics",
        "process_name"   : "PointOutputProcess",
        "Parameters"            : {
            "position"         :   [0.499, 0.25, 0.0],
            "model_part_name"  : "Structure",
            "output_file_settings": {
                "file_name"       : "node_output",
                "write_buffer_size" : 1
            },
            "output_variables" : ["DISPLACEMENT"],
            "entity_type"      : "node"
        }
    }]
    },
    "output_processes" : {
    	"data_generation":[{
			"python_module" : "data_generator_process",
			"kratos_module" : "NeuralNetworkApplication",
			"Parameters" 	: {
            "help"                     : "This process generates the data series for the neural network training and testing",
            "model_part_name"          : "Structure.GENERIC_Beam",
            "interval"                 : [0.0, "End"],
            "write_output_file"        : true,
            "input_variables"          : ["POINT_LOAD"],
            "input_sources"            : ["solution_step"],
            "input_model_part"         : "Structure.GENERIC_Beam",
            "output_variables"         : ["DISPLACEMENT_X","DISPLACEMENT_Y"],
            "output_sources"           : ["solution_step","solution_step"],
            "perturbate_variables"     : [],
            "random_distribution"      : ["normal"],
            "random_parameters"        : [[0,0.1]],
            "print_format"             : ".8f",
            "output_format"            : "ascii",
            "training_file_settings": {
                    "input_file_name"  : "training_in_raw",
                    "output_file_name" : "training_out_raw",
                    "file_format"      : "dat",
                    "output_path"      : "data/"
            }}
        }],
        "vtk_output" : [{
            "python_module" : "vtk_output_process",
            "kratos_module" : "KratosMultiphysics",
            "Parameters"    : {
                "model_part_name"                    : "Structure",
                "file_format"                        : "binary",
                "output_sub_model_parts"             : false,
                "output_interval"                    : 1,
                "nodal_solution_step_data_variables" : ["DISPLACEMENT","ROTATION","POINT_LOAD","POINT_MOMENT"],
                "gauss_point_variables_extrapolated_to_nodes" : []
            }
        }]
    }
}
