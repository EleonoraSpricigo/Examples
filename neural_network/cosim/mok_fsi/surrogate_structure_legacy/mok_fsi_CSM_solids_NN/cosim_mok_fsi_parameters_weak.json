{
    "problem_data" :
    {
        "start_time" : 0.0,
        "end_time" : 20.0,
        "echo_level" : 0,
        "print_colors" : true,
        "parallel_type" : "OpenMP"
    },
    "solver_settings" :
    {
        "type" : "coupled_solvers.gauss_seidel_weak",
        "echo_level" : 0,
        "predictors" : [
            {
                "type"                 : "linear_derivative_based",
                "solver"               : "fluid",
                "data_name"            : "disp",
                "derivative_data_name" : "velocity"
            }
        ],
        "data_transfer_operators" : {
            "mapper" : {
                "type" : "kratos_mapping",
                "mapper_settings" : {
                    "mapper_type" : "nearest_neighbor"
                }
            }
        },
        "coupling_sequence":
        [
            {
                "name": "fluid",
                "input_data_list"  : [],
                "output_data_list" : []
            },
            {
                "name": "neural_network",
                "input_data_list": [
                    {
                        "data"             : "load",
                        "from_solver"      : "fluid",
                        "from_solver_data" : "load",
                        "data_transfer_operator" : "mapper",
                        "data_transfer_operator_options" : ["swap_sign"]
                    }
                ],
                "output_data_list": [
                    {
                        "data"           : "disp",
                        "to_solver"      : "fluid",
                        "to_solver_data" : "disp",
                        "data_transfer_operator" : "mapper"
                    }
                ]
            }
        ],
        "solvers" :
        {
            "fluid":
            {
                "type" : "solver_wrappers.kratos.fluid_dynamics_wrapper",
                "solver_wrapper_settings" : {
                    "input_file"  : "ProjectParametersCFD"
                },
                "data" : {
                    "disp" : {
                        "model_part_name"   : "FluidModelPart.NoSlip2D_FSI",
                        "dimension" : 2,
                        "variable_name" : "MESH_DISPLACEMENT"
                    },
                    "load" : {
                        "model_part_name"   : "FluidModelPart.NoSlip2D_FSI",
                        "dimension" : 2,
                        "variable_name" : "REACTION"
                    },
                    "velocity" : {
                        "model_part_name"   : "FluidModelPart.NoSlip2D_FSI",
                        "dimension" : 2,
                        "variable_name" : "VELOCITY"
                    }
                }
            },
            "neural_network" :
            {
                "type" : "solver_wrappers.kratos.neural_network_with_mdpa_wrapper",
                "solver_wrapper_settings" : {
                    "input_file"  : "ProjectParametersNeuralNetwork",
                    "input_variable" : "load",
                    "output_variable": "disp",
                    "input_variable_name" : "FORCE",
                    "dimension_input"     : 2,
                    "output_variable_name": "DISPLACEMENT",
                    "mdpa_file_name" : "Mok_CSM",
                    "submodel_part_name":"GENERIC_FSI",
                    "lookback"       : 10,
                    "reorder_partitions": 4,
                    "features_as_timesteps": false,
                    "record"		: true,
                    "only_input"	: false,
                    "time_buffer"	: 1
                },
                "data" : {
                    "disp" : {
                        "model_part_name" : "NeuralNetwork.GENERIC_FSI",
                        "variable_name" : "DISPLACEMENT",
                        "location"      : "node_historical",
                        "dimension"     : 2
                    },
                    "load" : {
                        "model_part_name" : "NeuralNetwork.GENERIC_FSI",
                        "variable_name" : "FORCE",
                        "location"      : "node_historical",
                        "dimension"     : 2
                    }
                }
            }
        }
    }
}
