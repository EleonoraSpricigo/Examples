{   "problem_data":{
        "problem_type"  : "predict_with_modelpart",
        "parallel_type" : "OpenMP",
        "model_part_file": "Mok_CSM",
        "solver_type": "neural_network",
        "model_part_name": "Structure",
        "neural_network_file": "../only_structure/my_model",
        "external_model": true,
        "library_name": "keras",
        "import_applications": ["StructuralMechanicsApplication"],
        "input_model_part" : "Structure.GENERIC_FSI",
        "output_model_part": "Structure.GENERIC_FSI",
        "input_variables"          : ["POINT_LOAD_X", "POINT_LOAD_Y"],
