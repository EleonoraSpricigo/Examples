{
    "problem_data"             : {
        "problem_name"    : "Mok_CSM",
        "parallel_type"   : "OpenMP",
        "start_time"      : 0.0,
        "end_time"        : 15.0,
        "echo_level"      : 0
    },
    "co_sim_settings" : {
        "INFO" : "this block is only used if Kratos is used as an external solver for CoSimulation!",
        "is_strong_coupling" : true,
        "io_settings" : {
            "echo_level" : 0,
            "my_name" : "ext_structure",
            "connect_to" : "structure"
        },
        "communication_settings" : {
            "export_meshes" : [
                "Structure.GENERIC_FSI"
            ],
            "export_fields" : [{
                "identifier"      : "disp",
                "model_part_name" : "Structure.GENERIC_FSI",
                "variable_name"   : "DISPLACEMENT"
            }],
            "import_fields" : [{
                "identifier"      : "load",
                "model_part_name" : "Structure.GENERIC_FSI",
                "variable_name"   : "POINT_LOAD"
            }]
        }
    },
    "solver_settings"          : {
        "solver_type"                        : "dynamic",
        "echo_level"                         : 0,
        "analysis_type"                      : "non_linear",
        "time_integration_method"            : "implicit",
        "scheme_type"                        : "newmark",
        "model_part_name"                    : "Structure",
        "domain_size"                        : 2,
        "model_import_settings"              : {
            "input_type"     : "mdpa",
            "input_filename" : "Mok_CSM"
        },
        "material_import_settings"           : {
            "materials_filename" : "StructuralMaterials.json"
        },
        "time_stepping"               : {
            "time_step"           : 0.01
        },
        "line_search"                        : false,
        "convergence_criterion"              : "residual_criterion",
        "displacement_relative_tolerance"    : 1e-6,
        "displacement_absolute_tolerance"    : 1e-6,
        "residual_relative_tolerance"        : 1e-6,
        "residual_absolute_tolerance"        : 1e-6,
        "max_iteration"                      : 25,
        "linear_solver_settings"             : {
            "solver_type" : "LinearSolversApplication.sparse_qr"
        },
        "rotation_dofs"            : false
    },
    "processes" : {
        "constraints_process_list" : [{
            "python_module" : "assign_vector_variable_process",
            "kratos_module" : "KratosMultiphysics",
            "Parameters"    : {
                "model_part_name" : "Structure.DISPLACEMENT_Dirichlet",
                "variable_name"   : "DISPLACEMENT",
                "constrained"     : [true,true,true],
                "value"           : [0.0,0.0,0.0],
                "interval"        : [0.0,"End"]
            }
        }],
        "loads_process_list"       : [{
            "python_module" : "assign_vector_by_direction_to_condition_process",
            "kratos_module" : "KratosMultiphysics",
            "help"          : "This process sets a vector variable value over a condition",
            "check"         : "DirectorVectorNonZero direction",
            "process_name"  : "AssignModulusAndDirectionToConditionsProcess",
            "Parameters"            : {
                "mesh_id"         : 0,
                "model_part_name" : "Structure.PointLoad2D_FSI",
                "variable_name"   : "POINT_LOAD",
                "modulus"          : "1000*t*sin(0.001*t)",
                "direction"       : [1,0, 0],
                "interval"        : [0.0,"End"]
            }
        }],
        "list_other_processes"     : []
    },
    "output_processes" : {
    	"data_generation":[{
			"python_module" : "data_generator_process",
			"kratos_module" : "NeuralNetworkApplication",
			"Parameters" 	: {
            "help"                     : "This process generates the data series for the neural network training and testing",
            "model_part_name"          : "Structure.GENERIC_FSI",
            "interval"                 : [0.0, "End"],
            "write_output_file"        : true,
            "input_variables"          : ["POINT_LOAD"],
            "input_sources"            : ["condition"],
            "input_model_part"         : "Structure.PointLoad2D_FSI",
            "output_variables"         : ["DISPLACEMENT_X","DISPLACEMENT_Y"],
            "output_sources"           : ["solution_step","solution_step"],
            "perturbate_variables"     : [],
            "random_distribution"      : ["normal"],
            "random_parameters"        : [[100000,0.1]],
            "print_format"             : ".8f",
            "output_format"            : "ascii",
            "training_file_settings": {
                    "input_file_name"  : "training_in_raw",
                    "output_file_name" : "training_out_raw",
                    "file_format"      : "dat",
                    "output_path"      : "data/"
            }}
        }],
        "vtk_output" : [{
            "python_module" : "vtk_output_process",
            "kratos_module" : "KratosMultiphysics",
            "Parameters"    : {
                "model_part_name"                    : "Structure",
                "file_format"                        : "binary",
                "output_sub_model_parts"             : false,
                "output_interval"                    : 1,
                "nodal_solution_step_data_variables" : ["DISPLACEMENT","ROTATION","POINT_LOAD","POINT_MOMENT"],
                "gauss_point_variables_extrapolated_to_nodes" : []
            }
        }]
    }
}
